{
  "name": "logincontextapijwt",
  "version": "1.0.0",
  "description": "Node login system with react,jwt,exprees and contextAPI",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon sever.js",
    "client": "npm start --prefix client",
    "clientinstall": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Mateusz Świątek",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.0.0",
    "nodemon": "^2.0.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.8",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "jsonwebtoken": "^8.5.1",
    "materialize-css": "^1.0.0",
    "node-sass": "^4.13.0",
    "nodemailer": "^6.4.0",
    "pg": "^7.14.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  }
}
